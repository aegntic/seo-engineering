# Domain Configuration
DOMAIN=yourdomain.com

# MongoDB Configuration
MONGO_ROOT_USERNAME=your_mongo_user
MONGO_ROOT_PASSWORD=your_mongo_password

# n8n Configuration
N8N_HOST=your_n8n_host
N8N_PORT=5678
N8N_PROTOCOL=https
N8N_ENCRYPTION_KEY=your_n8n_encryption_key
DB_POSTGRESDB_PASSWORD=your_postgres_password

# JWT Secrets
JWT_ACCESS_SECRET=your_jwt_access_secret
JWT_REFRESH_SECRET=your_jwt_refresh_secret

# API Keys and Secrets
N8N_API_KEY=your_n8n_api_key
ENCRYPTION_KEY=your_encryption_key

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLIC_KEY=your_stripe_public_key
STRIPE_WEBHOOK_SECRET_WRITE=your_stripe_webhook_secret_write
STRIPE_WEBHOOK_SECRET_READ=your_stripe_webhook_secret_read

# Email Configuration
SMTP_HOST=smtp.yourprovider.com
SMTP_PORT=587
SMTP_USER=your_email_user
SMTP_PASS=your_email_password
SMTP_SENDER=noreply@yourdomain.com

# Monitoring
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin_password
